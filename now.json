{
    "version": 2,
    "name": "disqus",
    "alias": [
        ""
    ],
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "cache-control": "public, max-age=[Cache Age]"
            },
            "dest": "https://disqus.com/api/$1"
        }
    ],
    "github": {
        "enabled": true,
        "autoJobCancelation": true
    }
}
